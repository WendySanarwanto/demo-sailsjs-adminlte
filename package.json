{
  "name": "sails-adminlte",
  "private": true,
  "version": "0.0.0",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.18",
    "admin-lte": "^2.3.11",
    "bootstrap": "^3.3.7",
    "ejs": "2.3.4",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-coffee": "1.0.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-cssmin": "1.0.1",
    "grunt-contrib-jst": "1.0.0",
    "grunt-contrib-less": "1.3.0",
    "grunt-contrib-uglify": "1.0.1",
    "grunt-contrib-watch": "1.0.0",
    "grunt-sails-linker": "~0.10.1",
    "grunt-sync": "0.5.2",
    "include-all": "^1.0.0",
    "jquery": "^2.2.4",
    "rc": "1.0.1",
    "sails": "~0.12.13",
    "sails-disk": "~0.10.9",
    "ts-node": "^3.0.4"
  },
  "scripts": {
    "debug": "node debug app.js",
    "copy-bootstrap": "mkdir -p ./assets/libs/bootstrap && cp -r ./node_modules/bootstrap/dist/** ./assets/libs/bootstrap",
    "copy-admin-lte": "mkdir -p ./assets/libs/admin-lte && cp -r ./node_modules/admin-lte/dist/** ./assets/libs/admin-lte",
    "copy-admin-lte-plugins": "cp -r ./node_modules/admin-lte/plugins/ ./assets/libs/admin-lte",
    "copy-admin-lte-pages": "cp -r ./node_modules/admin-lte/pages/ ./assets/libs/admin-lte",
    "copy-frontend-assets": "npm run copy-bootstrap && npm run copy-admin-lte && npm run copy-admin-lte-plugins && npm run copy-admin-lte-pages",
    "prestart": "npm run copy-frontend-assets",
    "start": "node app.js"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/wendysa/sails-adminlte.git"
  },
  "author": "wendysa",
  "license": ""
}
